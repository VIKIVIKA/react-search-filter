import axios from 'axios';
import {UpdateJson} from '../../libs/UpdateJson';

const GET_TREEDATA = "GET_TREEDATA";
export function getTreeData(queryText){
	return (dispatch) => {
		var request = axios.get('/app/data/data.json');
		request.then(({data})=>{
						dispatch({type:GET_TREEDATA,payload:UpdateJson(data)});
					}).catch(function (error) {
						console.log(error);
					})
	}
}
